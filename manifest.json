{
  "name": "yapi-airship",
  "description": "基于yapi的接口代码自动化生成工具",
  "version": "0.0.0",
  "permissions": [
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://yapi.hellobike.cn/*",
        "http://yapi.hellobike.cn/*"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/index.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/hot-reload.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "测试工具",
    "default_icon": "asserts/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
