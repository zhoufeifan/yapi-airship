{
  "name": "M-API-CODE",
  "description": "接口代码自动化生成工具",
  "version": "0.0.0",
  "permissions": [
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/index.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/hot-reload.js"
    ],
    "persistent": false
  },
  "browser_action":
  {
    "default_title": "测试工具",
    "default_icon": "asserts/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}